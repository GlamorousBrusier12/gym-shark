generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://naveenkumar:password123@localhost:5432/gymshark?schema=public"
}

model exercise {
  name           String?
  target_muscles Json?
  type           String?
  is_deleted     Boolean?  @default(false)
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  deleted_at     DateTime? @db.Timestamp(6)
  video_url      Json?
  image_url      Json?
  id             Int       @id @default(autoincrement())
}

model exercise_map_exercise_property {
  id             Int       @id @default(autoincrement())
  fk_exercise_id Int?
  fk_property_id Int?
  is_deleted     Boolean?
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  deleted_at     DateTime? @db.Timestamp(6)

  @@index([fk_exercise_id, fk_property_id], map: "uniqe_fk_exercise_id_fk_property_id")
}

model exercise_property {
  id         Int       @id @default(autoincrement())
  name       String?
  value_type String?
  is_deleted Boolean?
  created_at DateTime? @db.Timestamp(6)
  updated_at DateTime? @db.Timestamp(6)
  deleted_at DateTime? @db.Timestamp(6)
}

model exercise_set {
  id             Int       @id @default(autoincrement())
  set_number     Int?
  fk_workout_id  Int?
  fk_exercise_id Int?
  value          Int?
  rpe            Int?
  type           String?
  is_deleted     Boolean?
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  deleted_at     DateTime? @db.Timestamp(6)

  @@index([set_number, fk_exercise_id, fk_workout_id], map: "unique_set_number_fk_exercise_id_fk_workout_id")
}

model map_exercise_workout {
  id             Int       @id @default(autoincrement())
  fk_workout_id  Int?
  fk_exercise_id Int?
  number_of_sets Int?
  is_deleted     Boolean?
  created_at     DateTime? @db.Timestamp(6)
  updated_at     DateTime? @db.Timestamp(6)
  deleted_at     DateTime? @db.Timestamp(6)

  @@index([fk_exercise_id, fk_workout_id], map: "unique_fk_exercise_id_fk_workout_id")
}

model map_set_exercise_property {
  id                      Int       @id @default(autoincrement())
  fk_set_id               Int?
  fk_exercise_property_id Int?
  is_deleted              Boolean?
  created_at              DateTime? @db.Timestamp(6)
  updated_at              DateTime? @db.Timestamp(6)
  deleted_at              DateTime? @db.Timestamp(6)
}

model user {
  id         Int       @id @default(autoincrement())
  username   String?   @db.VarChar
  email      String?
  password   String?
  logo       String?
  created_at DateTime? @db.Timestamp(6)
  updated_at DateTime? @db.Timestamp(6)
  is_deleted Boolean?
  deleted_at DateTime? @db.Timestamp(6)
}

model workout {
  id           Int       @id @default(autoincrement())
  name         String?
  duration     String?
  total_weight Int?
  fk_user_id   Int?
  is_deleted   Boolean?
  created_at   DateTime? @db.Timestamp(6)
  updated_at   DateTime? @db.Timestamp(6)
  deleted_at   DateTime? @db.Timestamp(6)
}

enum exercise_set_type {
  normal
  failure
  drop_set @map("drop-set")
  warm_up  @map("warm-up")
}
